procedure TfoldinginspFrm.BitBtn4Click(Sender: TObject);
var
  i : word;
  vpertama : boolean;
begin
  wwDBGrid3.ReadOnly:=TRUE;
  RekapOP.DisableControls;
  RekapOP.Close;
  //RekapOP.SetVariable('kd_transaksi',QTransaksiKD_TRANSAKSI.AsString);
  RekapOP.SetVariable('pawal',VTglAwal1.Date);
  RekapOP.SetVariable('pakhir',VTglAkhir1.Date);
  RekapOP.Open;
  RekapOP_Tot.Close;
  //RekapOP_Tot.SetVariable('kd_transaksi',QTransaksiKD_TRANSAKSI.AsString);
  RekapOP_Tot.SetVariable('pawal',VTglAwal1.Date);
  RekapOP_Tot.SetVariable('pakhir',VTglAkhir1.Date);
  RekapOP_Tot.Open;
  RekapOP.EnableControls;

  wwDBGrid3.ColumnByName('TOTAL').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapOP_TotTOTAL.AsFloat);
  wwDBGrid3.ColumnByName('AJL').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapOP_TotAJL.AsFloat);
  wwDBGrid3.ColumnByName('RAP').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapOP_TotRAP.AsFloat);
  wwDBGrid3.ColumnByName('SHUTTLE').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapOP_TotSHUTTLE.AsFloat);
  wwDBGrid3.ColumnByName('IHRAM').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapOP_TotIHRAM.AsFloat);
  wwDBGrid3.ColumnByName('FOLDING_SKT').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapOP_TotFOLDING_SKT.AsFloat);
  wwDBGrid3.ColumnByName('FOLDING_SHUT').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapOP_TotFOLDING_SHUT.AsFloat);
end;

procedure TfoldinginspFrm.VTglAkhir1Change(Sender: TObject);
begin
  if VTglAwal1.Date>VTglAkhir1.Date then
  begin
      ShowMessage('Tanggal Akhir harus LEBIH BESAR dari Tanggal Awal !');
      VTglAkhir1.Date:=VTglAwal1.Date;
  end;
end;

procedure TfoldinginspFrm.VTglAwal1Change(Sender: TObject);
begin
  VTglAkhir1.Date:=(VTglAwal1.Date);
end;

procedure TfoldinginspFrm.RekapOPAfterScroll(DataSet: TDataSet);
begin
{azmi}
    LabelBanner.Caption:='Record ke '+IntToStr(RekapOP.RecNo)+' dari '+FormatFloat('#,#',RekapOP.RecordCount)+' Records';
{azmi}
end;

procedure TfoldinginspFrm.BitBtn7Click(Sender: TObject);
begin
if RekapOP.Active then
          begin
             DMFrm.SaveDialog1.DefaultExt:='XLK';
             DMFrm.SaveDialog1.Filter:='Excel files (*.XLK)|*.XLK';
             DMFrm.SaveDialog1.FileName:=''+TabSheet4.Caption+' '+Caption+'';
             wwDBGrid3.ExportOptions.TitleName:=''+TabSheet4.Caption+' '+Caption+'';
               if DMFrm.SaveDialog1.Execute then
               begin
                 try
                 wwDBGrid3.ExportOptions.FileName:=DMFrm.SaveDialog1.FileName;
                 wwDBGrid3.ExportOptions.Save;
                 ShowMessage('Simpan Sukses !');
                 except
                 ShowMessage('Simpan Gagal !');
                 end;
               end;
          end
          else
            ShowMessage('Tabel belum di-OPEN !');
end;

procedure TfoldinginspFrm.BitBtn9Click(Sender: TObject);
var
  i : word;
  vpertama : boolean;
begin
  wwDBGrid4.BringToFront;
  RekapPROD.DisableControls;
  RekapPROD.Close;
  RekapPROD.SetVariable('pawal',VTglAwal11.Date);
  RekapPROD.SetVariable('pakhir',VTglAkhir11.Date);
  RekapPROD.Open;
  RekapPROD_Tot.Close;
  RekapPROD_Tot.SetVariable('pawal',VTglAwal11.Date);
  RekapPROD_Tot.SetVariable('pakhir',VTglAkhir11.Date);
  RekapPROD_Tot.Open;
  RekapPROD.EnableControls;

  wwDBGrid4.ColumnByName('TOTAL').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapPROD_TotTOTAL.AsFloat);
  wwDBGrid4.ColumnByName('AJL').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapPROD_TotAJL.AsFloat);
  wwDBGrid4.ColumnByName('RAP').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapPROD_TotRAP.AsFloat);
  wwDBGrid4.ColumnByName('SHUTTLE').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapPROD_TotSHUTTLE.AsFloat);
  wwDBGrid4.ColumnByName('IHRAM').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapPROD_TotIHRAM.AsFloat);
  wwDBGrid4.ColumnByName('FOLDING_SKT').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapPROD_TotFOLDING_SKT.AsFloat);
  wwDBGrid4.ColumnByName('FOLDING_SHUT').FooterValue:=FormatFloat('0.0,0;(0.0,0);- ',RekapPROD_TotFOLDING_SHUT.AsFloat);
end;

