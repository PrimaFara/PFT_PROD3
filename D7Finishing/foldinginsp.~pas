   //DMFrm.QRekanan.Close;
   //DMFrm.QRekanan.Open;
end;

procedure TfoldinginspFrm.QMasterBeforePost(DataSet: TDataSet);
begin
  if (QMasterISPOST.AsString='1') and (QMasterNO_NOTA.AsString='') then
  begin
      FNo_Urut.Close;
      FNo_Urut.Open;
      QMasterNO_NOTA.AsString:=FNo_UrutNO_NOTA.AsString;
  end;
end;

procedure TfoldinginspFrm.QDetailBeforeEdit(DataSet: TDataSet);
begin
  if QMasterISPOST.AsString='1' then
  begin
    ShowMessage('Maaf, sudah di-POSTING, tidak bisa diedit !...');
    Abort;
  end;

end;

procedure TfoldinginspFrm.Edit1KeyDown(Sender: TObject; var Key: Word;
  Shift: TShiftState);
begin
  if key=13 then
  begin
     QMaster.Close;
     QMaster.SetVariable('no_nota','%'+Edit1.Text+'%');
     QMaster.Open;
  end;
end;

procedure TfoldinginspFrm.wwDBNavigator1ButtonClick(Sender: TObject);
begin
  QDetail.Close;
  LookMaster.Execute;
  QDetail.Open;
end;

procedure TfoldinginspFrm.QDetailNewRecord(DataSet: TDataSet);
begin

DMFrm.QUserTime.Open;
QDetailTGL_INSERT.AsDateTime:=DMFrm.QUserTimeVTGL.AsDateTime;
QDetailNO_REG.AsInteger:=QMasterNO_REG.AsInteger;
QDetailKD_SUB_LOKASI.AsString:='-';

    QDetailQTY_GULUNG.AsFloat := 0;
    QDetailQTY_POTONG.AsFloat := 0;
    QDetailQTY_METER.AsFloat := 0;

    QDetailAJL.AsFloat := 0;
    QDetailRAP.AsFloat := 0;
    QDetailSHUTTLE.AsFloat := 0;
    QDetailIHRAM.AsFloat := 0;
    QDetailFOLDING_SKT.AsFloat := 0;
    QDetailFOLDING_SHUT.AsFloat := 0;

if wwDBComboBox1.Text = 'A' then
  begin
    QDetailGRUP_DET.AsString := 'A';
  end
else if wwDBComboBox1.Text = 'B' then
  begin
    QDetailGRUP_DET.AsString := 'B';
  end
else if wwDBComboBox1.Text = 'C' then
  begin
    QDetailGRUP_DET.AsString := 'C';
  end
else if wwDBComboBox1.Text = 'D' then
  begin
    QDetailGRUP_DET.AsString := 'D';
  end
else if wwDBComboBox1.Text = 'E' then
  begin
    QDetailGRUP_DET.AsString := 'E';
  end
else if wwDBComboBox1.Text = 'F' then
  begin
    QDetailGRUP_DET.AsString := 'F';
  end
else if wwDBComboBox1.Text = 'G' then
  begin
    QDetailGRUP_DET.AsString := 'G';
  end
else
  begin
    // Jika tidak ada yang sesuai, misalkan assign nilai default atau kosong
    QDetailGRUP_DET.AsString := '';
  end;


end;

procedure TfoldinginspFrm.BitBtn2Click(Sender: TObject);
begin
 DMFrm.QUserTime.Close;
 DMFrm.QUserTime.Open;
 if cbPreview.Checked then
   QuickRep1.Preview
   else
     QuickRep1.Print;
end;

