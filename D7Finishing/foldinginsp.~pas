{$R *.dfm}

procedure TfoldinginspFrm.FormClose(Sender: TObject;
  var Action: TCloseAction);
begin
  Action:=caFree;
  foldinginspFrm:=Nil;
end;

procedure TfoldinginspFrm.FormResize(Sender: TObject);
begin
  // PanelLeft.Width:=round((Width-PanelCenter.Width)/2)-4;
{  Caption:=IntToStr(width)+', '+IntToStr(PanelLeft.Width)+', '+IntToStr(PanelCenter.Width)+
    ', '+IntToStr(PanelRight.Width);    }
end;

procedure TfoldinginspFrm.wwDBGrid1Enter(Sender: TObject);
begin
  If QMaster.State<> dsBrowse then QMaster.Post;
end;

procedure TfoldinginspFrm.FormCreate(Sender: TObject);
begin
 QTransaksi.SetVariable('kd_transaksi','M05');
 QTransaksi.Open;
 foldinginspFrm.Caption:=QTransaksiNAMA_TRANSAKSI.AsString;
 QMaster.Open;
 QDetail.Open;
end;

