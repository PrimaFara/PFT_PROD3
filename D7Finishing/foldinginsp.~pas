procedure TfoldinginspFrm.WebBrowser1DocumentComplete(Sender: TObject;
  const pDisp: IDispatch; var URL: OleVariant);
var
  vaIn, vaOut: OleVariant;
begin
  WebBrowser1.ExecWB(OLECMDID_PRINT, OLECMDEXECOPT_PROMPTUSER, vaIn, vaOut);
end;

procedure TfoldinginspFrm.BitBtn11Click(Sender: TObject);
var
  HTMLFile: TStringList;
  FilePath: string;
  HTMLContent: string;
  TanggalStr: string;
begin
  WebBrowser1.BringToFront;
  FilePath := ExtractFilePath(Application.ExeName) + 'Laporan Produksi Folding.html';
  HTMLFile := TStringList.Create;
  try
    if VTglAwal11.Date = VTglAkhir11.Date then
      TanggalStr := FormatDateTime('dd mmmm yyyy', VTglAwal11.Date)
    else
      TanggalStr := FormatDateTime('dd mmmm yyyy', VTglAwal11.Date) + ' - ' + FormatDateTime('dd mmmm yyyy', VTglAkhir11.Date);
    HTMLContent := 
      '<!DOCTYPE html>' +
      '<html lang="id">' +
      '<head>' +
      '    <meta charset="UTF-8">' +
      '    <meta name="viewport" content="width=device-width, initial-scale=1.0">' +
      '    <title>Laporan Produksi Folding</title>' +
      '    <style> ' +
      '        body { font-family: Arial, sans-serif; font-size: 12px; } ' +
      '        .container { width: 100%; max-width: 210mm; padding: 10px; margin: auto; border: 1px solid #000; } ' +
      '        .header { width: 100%; text-align: left; margin-bottom: 10px; } ' +
      '        .header-table { width: 100%; border-collapse: collapse; } ' +
      '        .header-table td { padding: 4px; vertical-align: top; } ' +
      '        .logo { font-size: 18px; font-weight: bold; text-align: center; width: 20%; } ' +
      '        .logo-judul { font-size: 16px; font-weight: bold; text-align: center; width: 30%; } ' +
      '        .title { font-size: 14px; font-weight: bold; text-align: center; }' +
      '        .label { font-weight: bold; width: 40%; } ' +
      '        .received-from { font-weight: bold; margin-top: 10px; } ' +
      '        .order-table { margin-top: 5px; width: 80%; border-collapse: collapse; } ' +
      '        .order-table td { font-size: 12px; padding: 2px 5px; } ' +
      '        .table { width: 100%; border-collapse: collapse; margin-top: 10px; } ' +
      '        .table th, .table td { border: 1px solid #000; padding: 2px; text-align: center; } ' +
      '        .table th { background-color: #f0f0f0; } ' +
      '        .footer { margin-top: 15px; text-align: center; } ' +
      '        @media print { body { margin: 0; padding: 0; } .container { border: none; } @page { size: A4 portrait; margin: 10mm; } } ' +
      '        .footer-table { width: 100%; margin-top: 15px; border-collapse: collapse; } ' +
      '        .footer-table td { padding: 5px; border: none; font-weight: bold; } ' +
      '        .table tr:last-child td { border-bottom: 1px solid black; } ' +
      '    </style>' +
      '</head>' +
      '<body>' +
      '    <div class="container">' +
      '        <div class="header">' +
      '            <table class="header-table">' +
      '                <tr>' +
      '                    <td rowspan="2" class="logo">PT. PRIMAFARA TEXTILE</td>' +
      '                </tr>' +
      '                <tr>' +
      '                    <td class="logo-judul" colspan="3"><strong>REKAP PRODUKSI FOLDING</strong></td>' +
      '                </tr>' +
      '                <tr>' +
      '                    <td class="label" align="center">Sapugarut - Pekalongan</td>' +
      '                    <td class="label" align="center">Tanggal : ' + TanggalStr + '</td>' +
      '                    <td class="label" align="center">SATUAN : PCS</td>' +
      '                </tr>' +
      '            </table>' +
      '        </div>' +
      '        <table class="table">' +
      '            <tr>' +
      //'                <th>AJL</th>' +
      //'                <th>RAPIER</th>' +
      '                <th>SHUTTLE</th>' +
      '                <th>IHRAM</th>' +
      '                <th>FOLDING SKT JQ</th>' +
      '                <th>INSPECT PUTIHAN</th>' +
      '                <th>TOTAL</th>' +
      '                <th style="border: 0px; color: white;">1</th>' +
      '            </tr>';
      wwDBGrid4.DataSource.DataSet.First;
      while not wwDBGrid4.DataSource.DataSet.Eof do
      begin
      HTMLContent := HTMLContent +
      '            <tr>' +
      //'                <td>' + wwDBGrid4.DataSource.DataSet.FieldByName('AJL').AsString + '</td>' +
      //'                <td>' + wwDBGrid4.DataSource.DataSet.FieldByName('RAP').AsString + '</td>' +
      '                <td>' + wwDBGrid4.DataSource.DataSet.FieldByName('SHUTTLE').AsString + '</td>' +
      '                <td>' + wwDBGrid4.DataSource.DataSet.FieldByName('IHRAM').AsString + '</td>' +
      '                <td>' + wwDBGrid4.DataSource.DataSet.FieldByName('FOLDING_SKT').AsString + '</td>' +
      '                <td>' + wwDBGrid4.DataSource.DataSet.FieldByName('FOLDING_SHUT').AsString + '</td>' +
      '                <td>' + wwDBGrid4.DataSource.DataSet.FieldByName('TOTAL').AsString + '</td>' +
      '                <td style="border: 0px; color: white;">1</td>' +
      '            </tr>';
      wwDBGrid4.DataSource.DataSet.Next;
    end;

    HTMLContent := HTMLContent +
      '        </table>' +
      '        <div class="footer">' +
      '            <table class="footer-table">' +
      '                <tr>' +
      '                    <td align="center">Mengetahui</td>' +
      '                    <td align="center">Menyetujui</td>' +
      '                    <td align="center">Membuat</td>' +
      '                </tr>' +
      '                <tr>' +
      '                    <td style="height: 10px;"></td>' +
      '                    <td></td>' +
      '                    <td></td>' +
      '                    <td></td>' +
      '                </tr>' +
      '                <tr>' +
      '                    <td align="center">Salmin Ishaq</td>' +
      '                    <td align="center">Neka Faedah</td>' +
      '                    <td align="center">Fahrina</td>' +
      '                </tr>' +
      '            </table>' +
      '            <p style="text-align: center;"><small>1. Arsip, 2. Inspecting</small></p>' +
      '        </div>' +
      '    </div>' +
      '</body>' +
      '</html>';

    HTMLFile.Text := HTMLContent;
    HTMLFile.SaveToFile(FilePath);
    WebBrowser1.Navigate(FilePath);
  finally
    HTMLFile.Free;
  end;
end;

procedure TfoldinginspFrm.BtnFindClick(Sender: TObject);
begin
            wwDBGrid3.BringToFront;
            wwDBGrid3.ReadOnly:=false;

            if not RekapOP.QBEMode then
            begin
                wwDBGrid3.Options:=wwDBGrid3.Options-[dgRowSelect,dgAlwaysShowSelection];
                RekapOP.QBEMode:=TRUE;
            end
            else
                RekapOP.ClearQBE;
end;

procedure TfoldinginspFrm.BtnOk2Click(Sender: TObject);
var
    vt1, vt2, vt3, vt4, vt5, vt6, vt7: real;
begin
    wwDBGrid3.BringToFront;
    wwDBGrid3.ReadOnly:=TRUE;

    if RekapOP.QBEMode then
    begin
        RekapOP.ExecuteQBE;
        wwDBGrid3.Options:=wwDBGrid3.Options+[dgRowSelect,dgAlwaysShowSelection];
        //disini
        vt1 := 0; vt2 := 0; vt3 := 0; vt4 := 0; vt5 := 0; vt6 := 0; vt7 := 0;

        while not RekapOP.Eof do
            begin
                //vt1 := vt1 + RekapOPAJL.AsFloat;

                //vt2 := vt2 + RekapOPRAP.AsFloat;
                vt3 := vt3 + RekapOPSHUTTLE.AsFloat;
                vt4 := vt4 + RekapOPIHRAM.AsFloat;

                vt5 := vt5 + RekapOPFOLDING_SKT.AsFloat;
                vt6 := vt6 + RekapOPFOLDING_SHUT.AsFloat;
                vt7 := vt7 + RekapOPTOTAL.AsFloat;

                RekapOP.Next;
            end;

        //wwDBGrid3.ColumnByName('AJL').FooterValue := FormatFloat('0.0,0;(0.0,0);-', vt1);

        //wwDBGrid3.ColumnByName('RAP').FooterValue := FormatFloat('0.0,0;(0.0,0);-', vt2);
        wwDBGrid3.ColumnByName('SHUTTLE').FooterValue := FormatFloat('0.0,0;(0.0,0);-', vt3);
        wwDBGrid3.ColumnByName('IHRAM').FooterValue := FormatFloat('0.0,0;(0.0,0);-', vt4);

        wwDBGrid3.ColumnByName('FOLDING_SKT').FooterValue := FormatFloat('0.0,0;(0.0,0);-', vt5);
        wwDBGrid3.ColumnByName('FOLDING_SHUT').FooterValue := FormatFloat('0.0,0;(0.0,0);-', vt6);
        wwDBGrid3.ColumnByName('TOTAL').FooterValue := FormatFloat('0.0,0;(0.0,0);-', vt7);
    end            
end;

end.
