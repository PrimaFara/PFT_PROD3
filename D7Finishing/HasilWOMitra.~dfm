  object QLookBeam: TOracleDataSet
    SQL.Strings = (
      
        'select q.tanggal, q.no_seri_beam, q.konstruksi, q.corak,d.isi, q' +
        '.lokasi from ('
      
        'select b.tanggal, a.no_seri_beam, c.konstruksi, c.corak, a.prose' +
        's_tujuan||'#39' '#39'||a.lokasi_tujuan as lokasi'
      
        'from ipisma_db4.beam_mutasi_detail a, ipisma_db4.beam_mutasi b, ' +
        'ipisma_db4.beam_isi c'
      'where a.no_seri_beam=c.no_seri_beam and a.no_reg=b.no_reg and '
      
        'a.lokasi_tujuan=:plokasi and a.proses_tujuan=:pproses and a.kd_l' +
        'okasi='#39'50'#39' and a.kd_lokasi_org='#39'50'#39' and (a.lokasi_tujuan<>'#39'Turun' +
        ' Sanggan'#39') and b.ispost='#39'1'#39' and'
      'b.tanggal<=trunc(:ptgl)+1-1/86400'
      'order by tanggal desc ) q'
      
        'left outer join ipisma_db4.vprogres_tenun d on (q.no_seri_beam=d' +
        '.no_seri_beam)')
    Variables.Data = {
      0300000003000000080000003A504C4F4B415349050000000000000000000000
      080000003A5050524F534553050000000000000000000000050000003A505447
      4C0C0000000000000000000000}
    QBEDefinition.QBEFieldDefs = {
      04000000060000000700000054414E4747414C0100000000000C0000004E4F5F
      534552495F4245414D0100000000000A0000004B4F4E535452554B5349010000
      00000005000000434F52414B010000000000060000004C4F4B41534901000000
      000003000000495349010000000000}
    Session = DMFrm.OS
    Left = 368
    Top = 64
    object QLookBeamNO_SERI_BEAM: TStringField
